        static void Begin19()
        {
            int x1 = 10;
            int y1 = 2;
            int x2 = 5;
            int y2 = 1;
            int p = 2 * (Math.Abs(x1 - x2) + Math.Abs(y1 - y2));
            int s = Math.Abs(x1 - x2) * Math.Abs(y1 - y2);
            Console.WriteLine(p);
            Console.WriteLine(s);
        }
        static void Begin20()
        {
            int x1 = 5;
            int y1 = 2;
            int x2 = 10;
            int y2 = 4;
            int distance = Convert.ToInt32(Math.Sqrt(Math.Pow(x2 - x1, 2) + Math.Pow(y2 - y1, 2)));
            Console.WriteLine(distance);

        }
        static void Begin21()
        {
            int x1 = 20;
            int y1 = 8;
            int x2 = 10;
            int y2 = 4;
            int x3 = 5;
            int y3 = 2;
            int a = Convert.ToInt32(Math.Sqrt(Math.Pow(x1 - x2, 2) + Math.Pow(y1 - y2, 2)));
            int b = Convert.ToInt32(Math.Sqrt(Math.Pow(x2 - x3, 2) + Math.Pow(y2 - y3, 2)));
            int c = Convert.ToInt32(Math.Sqrt(Math.Pow(x3 - x1, 2) + Math.Pow(y3 - y1, 2)));
            int p = (a + b + c) / 2;
            int s = Convert.ToInt32(Math.Sqrt(p * (p - a) * (p - b) * (p - c)));
            Console.WriteLine(p);
            Console.WriteLine(s);
        }
