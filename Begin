            minus = Convert.ToInt32(Math.Pow(a, 2)) - Convert.ToInt32(Math.Pow(b, 2));
            multi = Convert.ToInt32(Math.Pow(a, 2)) * Convert.ToInt32(Math.Pow(b, 2));
            Console.WriteLine(sum);
            Console.WriteLine(minus);
            Console.WriteLine(multi);
        }
        static void Begin10_2()
