        static void Begin39()
        {
            int a = 6;
            int b = 3;
            int c = 9;
            int x1;
            int x2;
            int d = Convert.ToInt32(Math.Pow(b, 2) - 4 * a * c);
            x1 = -b + Convert.ToInt32(Math.Sqrt(d)) / (2 * a);
            x2 = -b - Convert.ToInt32(Math.Sqrt(d)) / (2 * a);
            if (x1 < x2)
            {
                Console.WriteLine(x1 + " " + x2);
            }
            else
            {
                Console.WriteLine(x2 + " " + x1);
            }
        }
        static void Begin40()
        {
            int a1 = 2;
            int b1 = 4;
            int c1 = 8;
            int a2 = 3;
            int b2 = 6;
            int c2 = 12;
            int d = (a1 * b2) - (a2 * b1);
            int x = (c1 * b2 - c2 * b1) / d;
            int y = (a1 * c2 - a2 * c1) / d;
            Console.WriteLine(x + " " + y);

        }
